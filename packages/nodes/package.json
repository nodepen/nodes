{
  "name": "@nodepen/nodes",
  "version": "2.0.0",
  "description": "node editor component library for nodepen",
  "author": "@cdriesler",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodepen/nodes.git"
  },
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "prebuild": "tailwindcss build -i tailwind.css -o dist/styles.css && copyfiles assets/*.ttf dist",
    "build": "tsc",
    "postbuild": "tsc-alias",
    "dev": "concurrently \"tsc-watch --noClear --onSuccess \\\"npm run prebuild\\\"\" \"tsc-alias -w\"",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "test": "jest"
  },
  "dependencies": {
    "@nodepen/core": "^2.0.0",
    "@speckle/viewer": "^2.16.0",
    "immer": "^10.0.2",
    "zustand": "4.4.1"
  },
  "peerDependencies": {
    "react": "^18.0",
    "react-dom": "^18.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "@types/three": "^0.155.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3"
  }
}
